	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(575.484741, 74.0570068, -509.887054, 0.999999166, -2.14755232e-12, -0.00130374334, 4.90762075e-12, 1, 2.11703055e-09, 0.00130374334, -2.11703499e-09, 0.999999166)
end)

