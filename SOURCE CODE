NoJumpCooldown.Name = "No Jump Cooldown"
NoJumpCooldown.Parent = Frame
NoJumpCooldown.BackgroundColor3 = Color3.fromRGB(17, 17, 17)
NoJumpCooldown.BorderColor3 = Color3.fromRGB(255, 255, 255)
NoJumpCooldown.Position = UDim2.new(0.510112345, 0, 0.231164411, 0)
NoJumpCooldown.Size = UDim2.new(0, 200, 0, 50)
NoJumpCooldown.Font = Enum.Font.LuckiestGuy
NoJumpCooldown.Text = "No Jump Cooldown"
NoJumpCooldown.TextColor3 = Color3.fromRGB(147, 0, 0)
NoJumpCooldown.TextSize = 14.000
NoJumpCooldown.MouseButton1Down:connect(function()
	--supports auto execute just put it in auto exc folder as a .txt
	if not game.IsLoaded(game) then 
		game.Loaded.Wait(game.Loaded);
	end

	local IsA = game.IsA;
	local newindex = nil 

	newindex = hookmetamethod(game, "__newindex", function(self, Index, Value)
		if not checkcaller() and IsA(self, "Humanoid") and Index == "JumpPower" then 
			return
		end

		return newindex(self, Index, Value);
	end)
end)

