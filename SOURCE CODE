FLOGO.BackgroundTransparency = 1.000
FLOGO.BorderColor3 = Color3.fromRGB(255, 255, 255)
FLOGO.Position = UDim2.new(0.269662917, 0, 0.457191765, 0)
FLOGO.Size = UDim2.new(0, 200, 0, 50)
